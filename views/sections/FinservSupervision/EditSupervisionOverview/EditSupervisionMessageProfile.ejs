<h5>
    <a href="#editSupervision-MessageProfile" style="color: lightseagreen; margin-left: 3em;" data-toggle="collapse" onclick="change_arrow($(this));">
        Message Sampling Profiles
        <i class="fa fa-caret-right" aria-hidden="true"></i>
    </a>
</h5>
<div id="editSupervision-MessageProfile" class="collapse">
    <div class="row">
        <div class="form-group col-4 response-cell">
            <label>Does the customer have different sampling rates for:</label>
        </div>
    </div>
    <div class="row">
        <div class="col-4, response-checkbox">
            <p>
                <input type="checkbox" name="finserv_supervision_questions[digital_comm][message_sampling_profiles][heightened_supervision]"
                    <%- finserv_supervision_questions.digital_comm.message_sampling_profiles.heightened_supervision
                    ? "checked" : "" %> />
                <b> Heightened Supervision</b>
            </p>
        </div>
        <div class="col-4, response-checkbox">
            <p>
                <input type="checkbox" name="finserv_supervision_questions[digital_comm][message_sampling_profiles][flagged_messages]" <%-
                    finserv_supervision_questions.digital_comm.message_sampling_profiles.flagged_messages
                    ? "checked" : "" %> />
                <b> Flagged Messages</b>
            </p>
        </div>
        <div class="col-4, response-checkbox">
            <p>
                <input type="checkbox" name="finserv_supervision_questions[digital_comm][message_sampling_profiles][clean]" <%- finserv_supervision_questions.digital_comm.message_sampling_profiles.clean
                    ? "checked" : "" %> />
                <b> Clean</b>
            </p>
        </div>
        <div class="col-4, response-checkbox">
            <p>
                <input type="checkbox" name="finserv_supervision_questions[digital_comm][message_sampling_profiles][content_type]" <%- finserv_supervision_questions.digital_comm.message_sampling_profiles.content_type
                    ? "checked" : "" %> />
                <b> Content Type</b>
            </p>
        </div>
        <div class="col-4, response-checkbox">
            <p>
                <input type="checkbox" name="finserv_supervision_questions[digital_comm][message_sampling_profiles][internal_messages]" <%-
                    finserv_supervision_questions.digital_comm.message_sampling_profiles.internal_messages
                    ? "checked" : "" %> />
                <b> Internal Messages</b>
            </p>
        </div>
        <div class="col-4, response-checkbox">
            <p>
                <input type="checkbox" name="finserv_supervision_questions[digital_comm][message_sampling_profiles][external_messages]" <%-
                    finserv_supervision_questions.digital_comm.message_sampling_profiles.external_messages
                    ? "checked" : "" %> />
                <b> External Messages</b>
            </p>
        </div>
    </div>

    <div class="row">
        <div class="form-group col-4 response-cell">
            <label>Heightened Supervision sampling rate</label>
            <input type="text" class="form-control" placeholder="No response set" value="<%= finserv_supervision_questions.digital_comm.message_sampling_profiles.heightened_supervision_rate %>"
                name="finserv_supervision_questions[digital_comm][message_sampling_profiles][heightened_supervision_rate]">
        </div>
        <div class="form-group col-4 response-cell">
            <label>Flagged Messages sampling rate</label>
            <input type="text" class="form-control" placeholder="No response set" value="<%= finserv_supervision_questions.digital_comm.message_sampling_profiles.flagged_messages_rate %>"
                name="finserv_supervision_questions[digital_comm][message_sampling_profiles][flagged_messages_rate]">
        </div>
        <div class="form-group col-4 response-cell">
            <label>Clean sampling rate</label>
            <input type="text" class="form-control" placeholder="No response set" value="<%= finserv_supervision_questions.digital_comm.message_sampling_profiles.clean_rate %>"
                name="finserv_supervision_questions[digital_comm][message_sampling_profiles][clean_rate]">
        </div>
        <div class="form-group col-4 response-cell">
            <label>Content Type sampling rate</label>
            <input type="text" class="form-control" placeholder="No response set" value="<%= finserv_supervision_questions.digital_comm.message_sampling_profiles.content_type_rate %>"
                name="finserv_supervision_questions[digital_comm][message_sampling_profiles][content_type_rate]">
        </div>
        <div class="form-group col-4 response-cell">
            <label>Internal Messages sampling rate</label>
            <input type="text" class="form-control" placeholder="No response set" value="<%= finserv_supervision_questions.digital_comm.message_sampling_profiles.internal_messages_rate %>"
                name="finserv_supervision_questions[digital_comm][message_sampling_profiles][internal_messages_rate]">
        </div>
        <div class="form-group col-4 response-cell">
            <label>External Messages sampling rate</label>
            <input type="text" class="form-control" placeholder="No response set" value="<%= finserv_supervision_questions.digital_comm.message_sampling_profiles.external_messages_rate %>"
                name="finserv_supervision_questions[digital_comm][message_sampling_profiles][external_messages_rate]">
        </div>
    </div>
</div>
